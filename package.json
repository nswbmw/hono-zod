{
  "name": "hono-zod",
  "version": "1.1.1",
  "description": "A simple validation middleware for Hono, using zod and zod-validation-error.",
  "author": "nswbmw",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "scripts": {
    "lint": "eslint .",
    "build": "tsup index.js --format esm,cjs",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "prepublishOnly": "npm run lint && npm run test && npm run build"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nswbmw/hono-zod.git"
  },
  "keywords": [
    "hono",
    "middleware",
    "zod",
    "validator",
    "validation",
    "validate",
    "schema",
    "error"
  ],
  "devDependencies": {
    "@hono/node-server": "1.19.1",
    "eslint": "8.57.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-n": "17.21.3",
    "eslint-plugin-promise": "6.1.1",
    "hono": "4.9.6",
    "jest": "30.1.3",
    "tsup": "8.5.0",
    "zod": "4.1.5"
  },
  "dependencies": {
    "zod-validation-error": "4.0.1"
  },
  "peerDependencies": {
    "zod": "4.1.5"
  }
}
